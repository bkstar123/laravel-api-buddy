{
    "name": "bkstar123/laravel-api-buddy",
    "description": "A lightweight Laravel package helps you to quickly build a high-quality RESTful API with transformation, filtering, sorting, paginating and more",
    "keywords": [
        "laravel",
        "api",
        "RESTful"
    ],
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Hoang Anh Tuan",
            "email": "hoangtuandhbk@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Bkstar123\\ApiBuddy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkstar123\\ApiBuddy\\ApiBuddyServiceProvider"
            ]
        }
    }
}
